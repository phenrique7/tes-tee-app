{
  "name": "tes-tee-app",
  "version": "0.1.0",
  "description": "A simple e-commerce page for a super fake shirt company",
  "author": "Paulo Henrique",
  "license": "MIT",
  "homepage": "https://github.com/phenrique7/tes-tee-app#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phenrique7/tes-tee-app.git"
  },
  "bugs": {
    "url": "https://github.com/phenrique7/tes-tee-app/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "^15.1.7",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^8.5.3",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "./**/*.js": [
      "./node_modules/.bin/eslint",
      "./node_modules/.bin/prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
